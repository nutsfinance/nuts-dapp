{
  "name": "nuts-dapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:zh": "ng serve --configuration=zh",
    "build": "ng build",
    "build:prod": "ng build --aot -vc -cc --build-optimizer --delete-output-path",
    "build:prod:zh": "ng build --aot -vc -cc --build-optimizer --delete-output-path --configuration=zh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n nuts-dapp --i18n-format xlf --output-path src/i18n --i18n-locale en && ng run nuts-dapp:xliffmerge",
    "start-zh": "ng serve --configuration=zh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
